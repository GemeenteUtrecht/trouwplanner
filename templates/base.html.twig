    	padding: .5em 1em;
        border: 2px solid #2A5587;
        background: #2A5587;
        color: #FFF !important;
        text-transform: uppercase;
        letter-spacing: .04em;
        font-weight: 700;
	}
	.btn-secondary {
    	padding: .5em 1em;
        border: 2px solid #2A5587;
        background: #FFF;
        color: #2A5587 !important;
        text-transform: uppercase;
        letter-spacing: .04em;
        font-weight: 700;
	}
	.btn-login {
    	padding: .5em 1em;
        border: 2px solid #FFF;
        background: #2A5587;
        color: #FFF !important;
        text-transform: uppercase;
        letter-spacing: .04em;
        font-weight: 700;
	}
	.btn-secondary:hover { 
        background: #FFF;
        color: #2A5587 !important;
	}
	.content {
        top: -20px;
        position: relative;
	}	
	.post-heading-left h2:after, .post-heading-center h2:after {
    background-color: #FFCC00;
	}
	.tbl-pricing .tbl-prc-col .tbl-prc-price {
	    background-color: #ffcc00;
	}
