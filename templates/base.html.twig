	/* header stuff */
	.header {    
    	background: #CC0000 url(/images/base-header-overlay.png) repeat top left;
        color: #fff;
        width: 100%;
        padding: 0;
        margin: 0;
        height: 200px;
	}
	.header-content {
    	position: relative;
    	left: 60px;
      	top: 50%;
      	transform: translateY(-50%);
      	-webkit-transform: translateY(-50%);
    }
    
    .header-content h1 {
    	font-size: 4em;
    	font-weight: 500;
    	font-family: 'Roboto', sans-serif;
        color: #fff;
    }
    
    .header-content h3 {
    	font-size: 4em;
    	font-weight: 100;
    	line-height: 0.6em;
    	margin-top: -5px;
    	opacity: 0.7;
    	font-family: 'Roboto', sans-serif;
        color: #fff;
    }
    
    .header-title {
    	display: table;
    }
    
    .header-icon {
    	font-size: 8em;	
    	float: left;
    	margin-right: 30px;
    	margin-top: 0px;
    }
    
    .header-menu {
    	position: relative;
    	margin: -100px auto 0;
        height: 100px;
        font-weight: 400;
    }
    
    .header-menu a:link{
    	color:#fff;
    }
    
    .header-menu a:hover{
    	color:#56c2b1;
    	text-decoration: none;
    }
    
    .header-menu a:visited{
    	color:#fff;
    }
    
    .menu-logo {
    	display: none;
    	color: #fff;
    	font-size: 0.25em;
    	position: absolute;
    	margin: -7px 0 0 -82px;
    }
    
    .mobile-header-menu .menu-logo {
    	margin: 2px 0 0 -19px;
    }
    
    .header-menu-content {
    	position: relative;
      	top: 50%;
      	transform: translateY(-50%);
      	-webkit-transform: translateY(-50%);
      	font-size: 1.2em;
      	padding-left: 65px;
    }
    
    .header-menu-content ul {
    	padding-left: 0;
    }
    
    .header-menu-content li {
    	display: inline;
    	list-style: none;
    	padding-right: 20px;
    }

	/* flow stuff*/
	#flow {
		padding-bottom: 20px;
	}
	
	.flow ul {
        display: flex;
        align-items: stretch; /* Default */
        justify-content: space-between;
        width: 100%;
        margin: 0;
        padding: 0;
    }
   .flow  li {
        display: block;
        flex: 0 1 auto; /* Default */
        list-style-type: none;
        color: #FFF;
        text-align: center;
  		text-transform: uppercase;
  		font-family: "Lucida Grande", "Lucida Sans Unicode", "Lucida Sans", Verdana, sans-serif;
    }
   .flow .fas {
	    background-color: #FFF;
        width: 90px;
        height: 90px;
        border-radius: 50%;
        text-align: center;
        line-height: 100px;
        vertical-align: middle;
		margin-bottom: 10px;
		margin-top: 5px;
		margin-left: 5px;
		margin-right: 5px;
    }
	.flow li a{
	    color: #2A5587;
	}
	.flow li a:hover .fas {
	    color: #CC0000;
        width: 100px;
        height: 100px;
		margin-bottom: 5px;
		margin-top: 0px;
		margin-left: 0px;
		margin-right: 0px;
	}
    
  

